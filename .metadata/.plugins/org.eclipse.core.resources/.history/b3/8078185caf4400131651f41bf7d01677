package GameClientStrategies;

public abstract class GameClientStrategy {
	
	private GameClientStrategies strategy;
	
	protected GameClientStrategy(){
		
	}
	
	public String getName(){
		return strategy.name();
	}


	public void setStrategy(GameClientStrategies strategy) {
		this.strategy = strategy;
	}
	public GameClientStrategies getStrategy() {
		return strategy;
	}
	

}
