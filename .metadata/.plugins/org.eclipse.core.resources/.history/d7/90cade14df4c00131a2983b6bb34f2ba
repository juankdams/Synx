package Providers;

import Interfaces.IProvider;

public class GameServerProvider implements IProvider {


	public GameServer createNew(short ID, short port, String dbHostIP, String dbName, String dbUser, String dbPass, String ownerss);
	
	public static class DofusLServerProvider implements LoginServerProvider {
		public GameServer createNew(short ID, short port, String dbHostIP, String dbName, String dbUser, String dbPass, String ownerss) {
			return new DofusGameServer(ID, port, dbHostIP, dbName, dbUser, dbPass, ownerss);
		}
	}
	public static class WakfuLServerProvider implements LoginServerProvider {
		public GameServer createNew(short ID, short port, String dbHostIP, String dbName, String dbUser, String dbPass, String ownerss) {
			return new WakfuGameServer(ID, port, dbHostIP, dbName, dbUser, dbPass, ownerss);
		}
	}
	
}
